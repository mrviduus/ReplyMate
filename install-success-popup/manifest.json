{
  "manifest_version": 3,
  "name": "Auto-Dismiss Success Popup",
  "version": "1.0.0",
  "description": "Chrome extension with auto-dismiss popup functionality",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Auto-Dismiss Popup"
  }
}
